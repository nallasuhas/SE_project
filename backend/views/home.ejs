<section id="section6" class="contact" style="margin-top: 50px">
  <div
    class="container"
    style="
      max-width: 1000px;
      margin: 0 auto;
      background-color: #a9a7a5;
      padding: 50px;
      border-radius: 10px;
    "
  >
    <div class="contact100-form-title" style="text-align: center">
      <!-- Keep In Touch Title -->
      <h3
        style="font-family: 'Arial', sans-serif; color: #333; font-size: 2.5rem"
      >
        Keep In Touch
      </h3>
      <!-- Send A Message Title -->
      <h2
        style="
          font-family: 'Arial', sans-serif;
          color: #333;
          font-size: 3rem;
          margin-top: 10px;
        ">
        Share your feedback :)
      </h2>
    </div>
    <form
      class="contact100-form validate-form"
      style="
        display: flex;
        flex-wrap: wrap;
        gap: 20px;
        font-family: 'Arial', sans-serif;
      "
    >
      <!-- Name Input -->
      <div
        class="wrap-input100 rs1-wrap-input100 validate-input"
        data-validate="Name is required"
        style="flex: 1; min-width: 250px"
      >
        <span class="label-input100" style="color: #333">Your Name</span>
        <input
          class="input100"
          type="text"
          name="name"
          placeholder="Enter your name"
          style="
            width: 100%;
            font-family: 'Arial', sans-serif;
            font-size: 1.2rem;
            border-radius: 15px;
            padding: 10px;
          "
        />
        <span class="focus-input100"></span>
      </div>

      <!-- Email Input -->
      <div
        class="wrap-input100 rs1-wrap-input100 validate-input"
        data-validate="Valid email is required: ex@abc.xyz"
        style="flex: 1; min-width: 250px"
      >
        <span class="label-input100" style="color: #333">Email</span>
        <input
          class="input100"
          type="text"
          name="email"
          placeholder="Enter email id"
          style="
            width: 100%;
            font-family: 'Arial', sans-serif;
            font-size: 1.2rem;
            border-radius: 15px;
            padding: 10px;
          "
        />
        <span class="focus-input100"></span>
      </div>

      <!-- Message Input -->
      <div
        class="wrap-input100 validate-input"
        data-validate="Message is required"
        style="width: 100%"
      >
        <span class="label-input100" style="color: #333">Message</span>
        <textarea
          class="input100"
          name="message"
          placeholder="your message here... "
          style="
            width: 100%;
            height: 150px;
            font-family: 'Arial', sans-serif;
            font-size: 1.2rem;
            border-radius: 15px;
            padding: 10px;
          "
        ></textarea>
        <span class="focus-input100"></span>
      </div>

      <!-- Submit Button -->
      <div
        class="container-contact100-form-btn"
        style="width: 100%; text-align: center"
      >
        <button
          class="contact100-form-btn"
          style="
            background-color: #ff6200;
            color: white;
            padding: 15px 30px;
            border: none;
            border-radius: 30px;
            box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
          "
        >
          <span>
            Submit
            <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
          </span>
        </button>
      </div>

      <!-- Error Message -->
      <div
        class="container-contact100-form-btn response"
        style="width: 100%; margin-top: 30px; text-align: center"
      >
        <p class="error"></p>
      </div>
    </form>
  </div>
</section>
<!-- Contact End -->

